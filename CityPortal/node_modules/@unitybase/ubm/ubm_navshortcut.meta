{
	"caption": "Shortcut",
	"description": "Shortcut to launch function",
	"connectionName": "",
	"sqlAlias": "navsh",
	"cacheType": "None", // we load cmdData on first click to prevent BLOB fetch
	"descriptionAttribute": "caption",
	"documentation": "",
	"attributes": {
		"desktopID": {
			"dataType": "Entity",
			"associatedEntity": "ubm_desktop",
			"caption": "Desktop",
			"description": "",
			"allowNull": false
		},
		"parentID": {
			"dataType": "Entity",
			"associatedEntity": "ubm_navshortcut",
			"caption": "Shortcut folder",
			"description": "",
			"allowNull": true
		},
		"code": {
			"dataType": "String",
			"size": 50,
			"caption": "Code",
			"documentation": "Unique shortcut code. Used for version upgrade",
			"allowNull": false,
			"isUnique": true
		},
		"isFolder": {
			"dataType": "Boolean",
			"caption": "Is this folder?",
			"allowNull": false,
			"defaultValue": "0"
		},
		"caption": {
			"dataType": "String",
			"size": 255,
			"caption": "Shortcut name",
			"allowNull": false,
			"isMultiLang": true
		},
		"cmdCode": {
			"dataType": "Text",
			"caption": "Command code"
		},
		"inWindow": {
			"dataType": "Boolean",
			"caption": "In new window",
			"description": "Display command result in new window or inside panel",
			"allowNull": false,
			"defaultValue": "0"
		},
		"isCollapsed": {
			"dataType": "Boolean",
			"caption": "Collapsed",
			"description": "Show collapsed at the first start",
			"documentation": "It make sense if isFolder = true",
			"allowNull": false,
			"defaultValue": "0"
		},
		"displayOrder":
		{
			"dataType": "Int",
			"caption": "№ order",
			"description": "Display order (in current folder)",
			"allowNull": false,
			"defaultValue": "1000"
		},
		"iconCls": {
			"dataType": "String",
			"size": 50,
			"caption": "icon css class name",
			"allowNull": true,
			"isMultiLang": false
		}
	},
	"mixins": {
		"mStorage": {
			"simpleAudit": true,
			"safeDelete" : true
		},
		"rls": {
			"expression": "([$.currentOwner()] OR 
					        [$.currentUserInGroup(ubm_navshortcut,'Admin')] OR
                            [$.currentUserOrUserGroupInAdmSubtable(ubm_navshortcut)])"
		},
		"tree": {
			"parentAttr": "parentID"
		}
	}
}
