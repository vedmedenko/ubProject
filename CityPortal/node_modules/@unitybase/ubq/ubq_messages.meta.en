        "msgData": {
            "caption": "Message data",
            "description": "Additional data for message. May contain Base64 encoded binary data"
        },
        "msgPriority": {
            "caption": "Priority",
            "description": "Priority of messages. 1=High, 0=Low, default 1"
        },
        "completeDate": {
            "caption": "Complete date"
        }
    }
}